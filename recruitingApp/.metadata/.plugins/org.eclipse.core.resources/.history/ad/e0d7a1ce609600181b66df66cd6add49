package com.peopleware.recruitingApp.models;

public class ItemDAO {

	private int id;
	private String product;

	public void setId(int id) {
		this.id = id;
	}

	public int getId() {
		return id;
	}

	public void setProduct(String product) {
		this.product = product;
	}

	public String getProduct() {
		return product;
	}

}
